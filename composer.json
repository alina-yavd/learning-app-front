{
  "name": "alina-yavd/learning-app-front",
  "description": "Learning App Front",
  "type": "project",
  "license": "MIT",
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.4",
    "nezamy/route": "dev-master",
    "symfony/asset": "^5.2@dev",
    "twig/twig": "3.x-dev"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16"
  },
  "autoload": {
    "psr-4": {
      "LearningApp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LearningApp\\Test\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "cs-check": "php-cs-fixer fix --allow-risky=yes --diff --ansi --dry-run",
    "cs-fix": "php-cs-fixer fix --allow-risky=yes --diff --ansi"
  }
}
